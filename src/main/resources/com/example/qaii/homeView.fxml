<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.qaii.controllers.homeController">
   <children>
      <Label fx:id="homeMenuLabel" layoutX="242.0" layoutY="23.0" text="Home Menu">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="appointmentButton" layoutX="242.0" layoutY="83.0" mnemonicParsing="false" onAction="#openAppointments" text="Appointments" />
      <Button fx:id="contactButton" layoutX="250.0" layoutY="136.0" mnemonicParsing="false" onAction="#openContacts" text="Customers" />
<!--      <Button fx:id="reportButton" layoutX="273.0" layoutY="252.0" mnemonicParsing="false" onAction="#openReports" text="Reports" />-->
      <Button id="logoutButton" fx:id="logoutButton" layoutX="525.0" layoutY="360.0" mnemonicParsing="false" onAction="#logoutButton" text="Log Out" />
      <Button fx:id="reportButton" layoutX="259.0" layoutY="200.0" mnemonicParsing="false" text="Reports" />
      <Text fx:id="notification" layoutX="277.0" layoutY="272.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   </children>
</AnchorPane>
